\mode<presentation>

%[vertical shading][bottom=green!5,top=blue!30]

\definecolor{cblue}{rgb}{.2353, .4902, .7725}
\definecolor{cdblue}{rgb}{.0745, .1882, .31760}

\setbeamercolor{structure}{fg=cblue}

\setbeamercolor{palette primary}{fg=white,bg=cdblue}
\setbeamercolor{palette secondary}{fg=white,bg=cblue}
\setbeamercolor{palette tertiary}{fg=cblue}

\setbeamercolor{block title}{use=structure,fg=structure.fg,bg=structure.fg!30!bg}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!50!bg}

\setbeamercolor{titlelike}{parent=palette tertiary}

\mode
<all>
